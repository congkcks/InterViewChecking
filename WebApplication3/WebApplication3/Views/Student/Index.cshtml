
@model IEnumerable<Student>
<a  asp-controller="Student" asp-action="Create" class="btn btn-primary">Create Student</a>
<a href="#" class="btn btn-primary">UpLoadAnh</a>
@if (Model != null)
{
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Mã</th>
                <th>Họ tên</th>
                <th>Ngày sinh</th>
                <th>Ngành</th>
                <th>Giới tính</th>
                <th>Hệ ĐT</th>
                <th>Địa chỉ</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var s in Model)
            {
                <tr>
                    <td>@s.Id</td>
                    <td>@s.Name</td>
                    <td>@s.DateOfBorth</td>
                    <td>@s.Branch</td>
                    <td>@s.Gender</td>
                    <td>@s.Name</td>
                    <td>@s.Address</td>
                    <td>@s.Email</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No students found.</p>
}
